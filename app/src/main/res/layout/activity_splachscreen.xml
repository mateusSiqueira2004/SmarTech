<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplachScreen">

    <ImageView
        android:id="@+id/logosplach"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:src="@drawable/logosemnome"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.273"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/titulosplach"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/smartech_comfortaa_negrito"
        app:layout_constraintEnd_toEndOf="@+id/logosplach"
        app:layout_constraintStart_toStartOf="@+id/logosplach"
        app:layout_constraintTop_toBottomOf="@+id/logosplach" />

</androidx.constraintlayout.widget.ConstraintLayout>